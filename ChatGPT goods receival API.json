{"openapi": "3.0.1", "info": {"title": "Goods Receival API", "description": "API for importing goods receival lists into the system.", "version": "1.0.0"}, "paths": {"/api/goodsreceivals/import": {"post": {"summary": "Import a Goods Receival list", "description": "Imports one goods receival list into the system.", "requestBody": {"required": true, "content": {"application/json": {"schema": {"type": "object", "properties": {"TransactionId": {"type": "integer", "format": "int32", "description": "Unique identifier for transaction"}, "PurchaseOrderId": {"type": "string", "maxLength": 50, "description": "Purchase order identifier", "example": "PO12345"}, "PurchaseOrderLineId": {"type": "integer", "format": "int32", "description": "Purchase order line number"}, "ExtProductId": {"type": "string", "maxLength": 50, "description": "Product number", "example": "PROD67890"}, "Quantity": {"type": "number", "format": "decimal", "description": "Quantity to place into warehouse", "example": 10.123}, "BatchId": {"type": "string", "maxLength": 50, "description": "Unique identifier for the batch", "example": "BATCH001"}, "ExpiryDate": {"type": "string", "maxLength": 50, "description": "Expiry date", "example": "2024-12-31"}, "Returned": {"type": "boolean", "description": "Indicates if inbound order line is returned goods. Values: 0 = not returned, 1 = returned", "example": false}}, "required": ["PurchaseOrderId", "PurchaseOrderLineId", "ExtProductId", "Quantity"]}}}}, "responses": {"200": {"description": "Successful import of goods receival list", "content": {"application/json": {"schema": {"type": "object", "properties": {"message": {"type": "string", "example": "Goods receival list imported successfully."}}}}}}, "400": {"description": "Invalid input, object invalid", "content": {"application/json": {"schema": {"type": "object", "properties": {"message": {"type": "string", "example": "Invalid input, object invalid"}}}}}}, "500": {"description": "Internal server error", "content": {"application/json": {"schema": {"type": "object", "properties": {"message": {"type": "string", "example": "Internal server error"}}}}}}}}}}}